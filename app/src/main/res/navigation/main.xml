    android:id="@+id/main"
    app:startDestination="@id/nav_profile">

    <fragment
        android:id="@+id/nav_history"
        android:name="com.example.ParkirYuk.HistoryFragment"
        android:label="History"
        tools:layout="@layout/fragment_history" />
    <fragment
