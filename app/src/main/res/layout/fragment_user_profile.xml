<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.example.ParkirYuk.userProfileFragment"
    android:background="@color/colorWhite">

    <androidx.constraintlayout.widget.ConstraintLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent">

        <TextView
            android:id="@+id/getUsername"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="30dp"
            android:fontFamily="@font/montserrat_light"
            android:textColor="@color/colorBlack"
            android:textSize="24sp"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/getEmail"
            tools:text="Username" />

        <TextView
            android:id="@+id/getEmail"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="40dp"
            android:fontFamily="@font/montserrat_light"
            android:textColor="@color/colorBlack"
            android:textSize="24sp"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            tools:text="Email Address" />

        <TextView
            android:id="@+id/TextviewBox"
            android:layout_width="260dp"
            android:layout_height="250dp"
            android:layout_marginTop="30dp"
            android:background="@drawable/box"
            app:layout_constraintTop_toBottomOf="@id/getUsername"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintEnd_toEndOf="parent"/>

        <TextView
            android:id="@+id/head"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="History"
            android:fontFamily="@font/montserrat_light"
            android:textSize="20sp"
            android:textColor="@color/colorBlack"
            android:layout_marginTop="20dp"
            app:layout_constraintTop_toTopOf="@id/TextviewBox"
            app:layout_constraintStart_toStartOf="@id/TextviewBox"
            app:layout_constraintEnd_toEndOf="@id/TextviewBox"/>

        <TextView
            android:id="@+id/namePlaceHistory"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="mall taman anggrek"
            android:layout_marginTop="20dp"
            android:layout_marginStart="8dp"
            android:layout_marginLeft="8dp"
            android:textSize="12sp"
            android:textColor="@color/colorBlack"
            app:layout_constraintTop_toBottomOf="@id/head"
            app:layout_constraintStart_toStartOf="@id/countHistory"/>

        <TextView
            android:id="@+id/countHistory"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="1"
            android:layout_marginTop="20dp"
            android:layout_marginStart="15dp"
            android:layout_marginLeft="15dp"
            android:textSize="12sp"
            android:textColor="@color/colorBlack"
            app:layout_constraintTop_toBottomOf="@id/head"
            app:layout_constraintStart_toStartOf="@id/TextviewBox"/>

        <TextView
            android:id="@+id/orHistory"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="20dp"
            android:text="/"
            android:textSize="12sp"
            app:layout_constraintTop_toBottomOf="@id/head"
            app:layout_constraintStart_toEndOf="@id/namePlaceHistory"
            app:layout_constraintEnd_toEndOf="@id/TextviewBox"/>

        <TextView
            android:id="@+id/maxHistory"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="5dp"
            android:layout_marginLeft="5dp"
            android:layout_marginTop="20dp"
            android:fontFamily="@font/montserrat_light"
            android:textColor="@color/colorBlack"
            android:textSize="12sp"
            android:text="50"
            app:layout_constraintTop_toBottomOf="@id/head"
            app:layout_constraintStart_toEndOf="@id/orHistory"/>

        <TextView
            android:id="@+id/currHistory"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="20dp"
            android:layout_marginRight="5dp"
            android:fontFamily="@font/montserrat_light"
            android:textColor="@color/colorBlack"
            android:textSize="12sp"
            app:layout_constraintTop_toBottomOf="@id/head"
            android:text="20"
            app:layout_constraintEnd_toStartOf="@id/orHistory"/>

    </androidx.constraintlayout.widget.ConstraintLayout>

</FrameLayout>